{
  "name": "@heys/web",
  "version": "13.0.1",
  "private": true,
  "description": "HEYS Web Application - Main web interface",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "clean": "rimraf dist",
    "dev": "cross-env PORT=3001 vite",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "preview": "cross-env PORT=3001 vite preview",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:watch": "vitest --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@heys/analytics": "workspace:*",
    "@heys/core": "workspace:*",
    "@heys/gaming": "workspace:*",
    "@heys/search": "workspace:*",
    "@heys/shared": "workspace:*",
    "@heys/storage": "workspace:*",
    "@heys/ui": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "typescript": "^5.2.0",
    "vite": "^4.4.0",
    "vitest": "^0.34.0"
  }
}
