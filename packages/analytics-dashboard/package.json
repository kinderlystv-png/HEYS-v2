{
  "name": "@heys/analytics-dashboard",
  "version": "0.1.0",
  "description": "Business Intelligence MVP - Real-time analytics dashboard for HEYS v2",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "dev": "vite --config vite.config.dev.ts",
    "demo": "vite --config vite.config.dev.ts",
    "build:demo": "vite build --config vite.config.dev.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:visual": "playwright test",
    "test:visual:ui": "playwright test --ui",
    "test:visual:headed": "playwright test --headed",
    "test:visual:update": "playwright test --update-snapshots",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@types/d3": "^7.4.3",
    "buffer": "^6.0.3",
    "d3": "^7.9.0",
    "date-fns": "^3.6.0",
    "eventemitter3": "^5.0.1",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.7",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.0",
    "jsdom": "^24.1.1",
    "tsup": "^8.2.4",
    "typescript": "^5.9.2",
    "vite": "^5.4.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@heys/shared": "workspace:*"
  },
  "keywords": [
    "analytics",
    "dashboard",
    "business-intelligence",
    "real-time",
    "metrics",
    "heys"
  ],
  "author": "HEYS v2 Development Team",
  "license": "MIT"
}
