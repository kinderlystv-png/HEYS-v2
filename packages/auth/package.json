{
  "name": "@heys/auth",
  "version": "1.0.0",
  "description": "Authentication service for HEYS EAP 3.0",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@heys/shared": "workspace:^",
    "@supabase/supabase-js": "^2.38.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^19.1.9",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "keywords": [
    "auth",
    "authentication",
    "supabase",
    "react",
    "typescript"
  ],
  "author": "HEYS Team",
  "license": "MIT"
}
